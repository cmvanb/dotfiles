#-------------------------------------------------------------------------------
# Workspace configuration for laptop
#-------------------------------------------------------------------------------

set $laptop eDP-1

workspace 1 output $laptop
workspace 2:2 output $laptop
workspace 3:3 output $laptop
workspace 4:4 output $laptop
workspace 5:5 output $laptop
workspace 6:6 output $laptop
workspace 7:7 output $laptop
workspace 8:8 output $laptop
workspace 9:9 output $laptop
workspace 10:10 output $laptop
workspace home output $laptop
workspace work output $laptop
workspace todo output $laptop
workspace spotify output $laptop
workspace auth output $laptop

exec swaymsg 'workspace home; exec qutebrowser -r home'
exec swaymsg workspace 1
